import java.util.Scanner;

public class EcuacionCuadratica {
    private double a;
    private double b;
    private double c;

    public EcuacionCuadratica(double a, double b, double c) {
        this.a = a;
        this.b = b;
        this.c = c;
    }

    public double getDiscriminante() {
        return b * b - 4 * a * c;
    }

    public double getRaiz1() {
        double d = getDiscriminante();
        if (d < 0) return 0;
        return (-b + Math.sqrt(d)) / (2 * a);
    }

    public double getRaiz2() {
        double d = getDiscriminante();
        if (d < 0) return 0;
        return (-b - Math.sqrt(d)) / (2 * a);
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("Ingrese a: ");
        double a = sc.nextDouble();

        System.out.print("Ingrese b: ");
        double b = sc.nextDouble();

        System.out.print("Ingrese c: ");
        double c = sc.nextDouble();

        EcuacionCuadratica eq = new EcuacionCuadratica(a, b, c);

        double discriminante = eq.getDiscriminante();

        if (discriminante > 0) {
            System.out.println("La ecuación tiene dos raices " + eq.getRaiz1() + " y " + eq.getRaiz2());
        } else if (discriminante == 0) {
            System.out.println("La ecuación tiene una raiz " + eq.getRaiz1());
        } else {
            System.out.println("La ecuacion no tiene raices reales");
        }

        sc.close();
    }
}
